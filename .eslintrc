{
  "extends": "airbnb-base",
  "rules": {
    "import/newline-after-import": "off",
    "import/extensions": "off",
    "object-curly-newline": "off",
    "class-methods-use-this": "off",
    "consistent-return": "off",
    "linebreak-style": [
      "error",
      "windows"
    ],
    "no-shadow": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "log"
        ]
      }
    ],
    "quotes": [
      "error",
      "double"
    ]
  }
}